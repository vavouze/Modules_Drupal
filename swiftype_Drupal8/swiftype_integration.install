<?php

/**
 * @file
 * Swiftype integration module install functions.
 */

/**
 * Implements hook_uninstall().
 */
function swiftype_integration_uninstall() {
  variable_del('swiftype_integration_install_key');
  variable_del('swiftype_integration_api_key');
  variable_del('swiftype_integration_engine_key');
  variable_del('swiftype_integration_ct');
}
